{
  "name": "@viseo/sea",
  "version": "1.0.0",
  "description": "Swagger-UI + Express + AJV",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && babel src --ignore ./src/Test --out-dir dist --copy-files",
    "build:watch": "watch 'npm run build' src",
    "lint": "eslint --ignore-path .eslintignore .",
    "coverage": "nyc --exclude=**/*.spec.js --skip-empty --skip-full --reporter=html --reporter=text npm run test",
    "test": "find . -not \\( -path ./dist -prune \\) -name '*.ssc.spec.js' | xargs mocha --require @babel/register --require @babel/polyfill",
    "seagen": "node ./src/App/generator.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/viseo-digital-squads/proof-of-concepts/sea"
  },
  "keywords": [
    "viseo",
    "sea",
    "express",
    "ajv",
    "swagger-ui"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "watch": "^1.0.2"
  },
  "bin": {
    "seagen": "./dist/App/generator.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "ajv": "^6.10.0",
    "ajv-errors": "^1.0.1",
    "async": "^3.0.1",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "compression": "^1.7.4",
    "core-js": "^3.1.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "fs-extra": "^8.0.1",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "prompt": "^1.0.0",
    "shelljs": "^0.8.3",
    "swagger-parser": "^6.0.5",
    "swagger-ui-dist": "^3.22.2",
    "version-healthcheck": "^0.1.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0"
  }
}
